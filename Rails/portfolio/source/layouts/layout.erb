<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Thanh Nguyen" %></title>
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.2.0/css/font-awesome.min.css" rel="stylesheet">
    
    <%= stylesheet_link_tag "normalize", "all", "agency", "bootstrap", "bootstrap.min" %>
    <%= javascript_include_tag  "all", "agency", "bootstrap", "bootstrap.min", "cbpAmimatedHeader", "cbpAnimatedHeader.min", "classie", "contact_me", "jqBootstrapValidation", "jquery-1.11.0" %>
  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>